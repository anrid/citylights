    "@babel/preset-react": "^7.27.1",
    "assets-webpack-plugin": "^7.1.1",
    "aws-sdk": "^2.1692.0",
    "babel-loader": "^10.0.0",
    "code": "^5.2.4",
    "css-loader": "^7.1.2",
    "expect": "^29.7.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^2.1.3",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.1",
    "lab": "^18.0.2",
    "mimer": "^2.0.2",
    "mocha": "^11.4.0",
    "node-sass": "^9.0.0",

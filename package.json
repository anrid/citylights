    "css-loader": "^0.23.1",
    "expect": "^1.20.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
